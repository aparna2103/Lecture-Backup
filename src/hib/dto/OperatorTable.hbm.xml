<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    <class name="OperatorTable" table="opTab">
    <id name="opId" column="operator_Id"/>
    <property name="opName" column="operator_name"/>
    <property name="opPassword" column="operator_pass"/>
    <property name="opContactNo" column="operator_Contact"/>
    <property name="opMailId" column="operator_mail_id"/>
    </class>
</hibernate-mapping>
